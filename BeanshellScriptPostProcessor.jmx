        <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="script">import java.io.*;

// Define the file path
File f = new File(&quot;C:\\Users\\sathy\\OneDrive\\Desktop\\jmeter\\TestResults.csv&quot;);

// Initialize FileWriter and BufferedWriter
FileWriter fw = new FileWriter(f, true);  // &apos;true&apos; to append data
BufferedWriter bw = new BufferedWriter(fw);

// Get the response code
String rc = prev.getResponseCode();

try {
    // Write test result based on response code
    if(rc.equals(&quot;200&quot;)){
        bw.write(&quot;Test is Passed&quot;);
    } else {
        bw.write(&quot;Test is Failed&quot;);
    }
    bw.newLine(); // Ensure each result is on a new line

    // Flush the buffer
    bw.flush();
} catch (IOException e) {
    log.error(&quot;Error writing to file: &quot; + e.getMessage());
} finally {
    // Close the BufferedWriter to ensure data is written to the file
    bw.close();
    fw.close();
}
</stringProp>
